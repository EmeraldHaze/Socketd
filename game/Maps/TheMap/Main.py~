story = 'Once upon a day, you were a-walking in a forest, when you acedently steped on some railroads, upon which you were driven, against your will, to a cave and thrown within.'
l = 8
startt = 'B7'
from Maps.TheMap.Types import *
class Place:
        def __init__(self, Type, dirlinks, cordlinks, **kwargs):
                self.cordlinks = cordlinks
                self.dirlinks = dirlinks
                if Type == 'sp':
                        self.look = kwargs['sp']
                else:
                        self.look = types[Type]
                self.exits = 'Exits: '
                for i in cordlinks:
                        self.exits += i+', '
                self.exits = self.exits[:-2]
                if 'end' in kwargs:
                        self.end = True
                else:
                        self.end = False




